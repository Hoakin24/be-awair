{
  "cli": {
    "version": ">= 12.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "GOOGLE_MAPS_API_KEY": "$GOOGLE_MAPS_API_KEY",
        "OPENWEATHER_API_KEY": "$OPENWEATHER_API_KEY"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "GOOGLE_MAPS_API_KEY": "$GOOGLE_MAPS_API_KEY",
        "OPENWEATHER_API_KEY": "$OPENWEATHER_API_KEY"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
